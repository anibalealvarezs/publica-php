{
    "name": "anibalealvarezs/publica-php",
    "description": "Publica.la API Connector for PHP",
    "version": "0.0.1",
    "keywords": [
        "publica",
        "php",
        "sdk",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aníbal Álvarez",
            "email": "anibalealvarezs@gmail.com",
            "homepage": "https://anibalalvarez.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "guzzlehttp/guzzle": "^6.4 || ^7.2"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PublicaPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PublicaPHP\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}